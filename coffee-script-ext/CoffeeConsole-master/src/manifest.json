{
	"name": "CoffeeConsole",
	"version": "0.0.3",
	"description": "A CoffeeScript console within the Web Inspector.",
	"devtools_page": "coffee.html",
    "icons": { "128": "128.png" },
	"minimum_chrome_version" : "18",
	"permissions": [
		"tabs",
		"contextMenus"
	],
	"content_scripts": [
	{
		"matches": ["<all_urls>"],
		"js": ["lookeelookee.js"],
		"run_at": "document_start"
	}
],
	"manifest_version": 2
}
